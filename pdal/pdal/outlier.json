{
  "pipeline":[
    "/data/{}.laz",
    {
      "type":"filters.outlier",
      "method":"statistical",
      "mean_k":12,
      "multiplier":2.2
    },
    {
      "type":"filters.range",
      "limits":"Classification![7:7]"
    },
    {
      "type":"writers.las",
      "filename":"/filtered/{}.laz"
    }
  ]
}